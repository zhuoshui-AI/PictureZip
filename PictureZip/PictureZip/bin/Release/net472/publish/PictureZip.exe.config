<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="ImageCompressorSettings" type="ImageCompressor.Settings.ImageCompressorSettingsSection, ImageCompressor" />
  </configSections>
  <ImageCompressorSettings>
    <!-- 默认压缩质量 (0-100) -->
    <DefaultQuality value="80" />
    <!-- 默认输出格式 (JPEG, PNG, BMP, GIF) -->
    <DefaultFormat value="JPEG" />
    <!-- 默认缩放比例 (百分比) -->
    <DefaultScale value="100" />
    <!-- 是否记住上次使用的目录 -->
    <RememberLastDirectory value="true" />
    <!-- 上次使用的目录 -->
    <LastUsedDirectory value="" />
    <!-- 窗口位置和大小 -->
    <WindowSettings x="100" y="100" width="600" height="400" />
    <!-- 支持的图片格式 -->
    <SupportedFormats>
      <Format extension=".jpg" description="JPEG 图片" />
      <Format extension=".jpeg" description="JPEG 图片" />
      <Format extension=".png" description="PNG 图片" />
      <Format extension=".bmp" description="BMP 图片" />
      <Format extension=".gif" description="GIF 图片" />
      <Format extension=".tiff" description="TIFF 图片" />
    </SupportedFormats>
  </ImageCompressorSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
  </startup>
</configuration>